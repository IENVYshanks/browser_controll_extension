{
  "manifest_version": 3,
  "name": "User Input Extension",
  "version": "1.0",
  "description": "Send user input to background.js and display the response",
  "permissions": ["storage","sidePanel","tabs"],
  "background": {
    "service_worker": "background.js"
  },
    "side_panel": {
    "default_path": "popup.html"
    },
  "action": {
    "default_icon": {
      "128": "image.jpg"
    }
  }
}
