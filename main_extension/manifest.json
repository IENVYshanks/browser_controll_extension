{
  "manifest_version": 3,
  "name": "User Input Extension",
  "version": "1.0",
  "description": "Send user input to background.js and display the response",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "image.jpg"
    }
  }
}
